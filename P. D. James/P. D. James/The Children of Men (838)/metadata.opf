        <dc:title>The Children of Men</dc:title>
        <dc:creator opf:file-as="James, P. D." opf:role="aut">P. D. James</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
