        <dc:title>Between a Heart and a Rock Place: A Memoir</dc:title>
        <dc:creator opf:file-as="Benatar, Pat" opf:role="aut">Pat Benatar</dc:creator>
        <dc:creator opf:file-as="Benatar, Pat" opf:role="aut">Patsi Bale Cox</dc:creator>
